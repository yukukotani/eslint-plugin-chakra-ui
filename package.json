{
  "name": "eslint-plugin-chakra-ui",
  "version": "0.7.5",
  "description": "ESLint Plugin for Chakra UI",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "Chakra UI",
    "chakra-ui"
  ],
  "author": "Yuku Kotani (yukukotani@gmail.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/yukukotani/eslint-plugin-chakra-ui"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .ts,.tsx ./src",
    "lint:tsc": "tsc --noEmit",
    "lint:prettier": "prettier --check ./src",
    "fix:prettier": "prettier --write ./src",
    "build": "rm -rf ./dist && swc ./src -d dist",
    "test": "uvu -r @swc-node/register src/__tests__",
    "test:watch": "watchlist src -- pnpm run test",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^5.44.0"
  },
  "devDependencies": {
    "@swc-node/register": "^1.5.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.4",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@typescript-eslint/types": "^5.44.0",
    "eslint": "^8.24.0",
    "eslint-plugin-eslint-plugin": "^5.0.6",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3",
    "uvu": "^0.5.6",
    "watchlist": "^0.3.1"
  },
  "engines": {
    "node": "12.x || 14.x || >= 16",
    "pnpm": ">=7.9.3"
  },
  "peerDependencies": {
    "eslint": ">=6"
  },
  "license": "MIT",
  "files": [
    "dist"
  ]
}
